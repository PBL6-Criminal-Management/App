{
  "expo": {
    "name": "Criminal-management-app",
    "slug": "Criminal-management-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Cho phép $(PRODUCT_NAME) để truy cập ảnh của bạn",
        "NSPhotoLibraryAddUsageDescription": "Cho phép $(PRODUCT_NAME) để lưu ảnh của bạn"
      },
      "bundleIdentifier": "com.anonymous.Criminalmanagementapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ],
      "package": "com.anonymous.Criminalmanagementapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Cho phép $(PRODUCT_NAME) để truy cập camera của bạn"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Cho phép $(PRODUCT_NAME) để truy cập ảnh của bạn",
          "savePhotosPermission": "Cho phép $(PRODUCT_NAME) để lưu ảnh của bạn",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0fb524f3-40b4-4f28-90e7-7eb0ff782144"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0fb524f3-40b4-4f28-90e7-7eb0ff782144"
    }
  }
}